# mount point       fstype    device                                                                flags
# From TingyiChen (https://github.com/TingyiChen/android_device_xiaomi_FYJ01QP-twrp/blob/android-8.1/recovery.fstab)

/system			ext4	/dev/block/platform/bootdevice/by-name/system
/system_image		emmc	/dev/block/platform/bootdevice/by-name/system		flags=display="system_image";backup=1;flashimg=1
/boot			emmc	/dev/block/platform/bootdevice/by-name/boot		flags=backup=1
/recovery		emmc	/dev/block/platform/bootdevice/by-name/recovery		flags=backup=1
/vendor			ext4    /dev/block/platform/bootdevice/by-name/vendor		flags=backup=1
/vendor_image		emmc    /dev/block/platform/bootdevice/by-name/vendor		flags=display="vendor_image";backup=1;flashimg=1
/data			ext4    /dev/block/platform/bootdevice/by-name/userdata		flags=forceencrypt=/dev/block/platform/bootdevice/by-name/metadata
/boot_para		emmc    /dev/block/platform/bootdevice/by-name/boot_para	flags=backup=1
/proinfo		emmc	/dev/block/platform/bootdevice/by-name/proinfo		flags=backup=1
/lk			emmc	/dev/block/platform/bootdevice/by-name/lk		flags=backup=1
/lk2			emmc	/dev/block/platform/bootdevice/by-name/lk2		flags=backup=1;subpartitionof=/lk
/para			emmc	/dev/block/platform/bootdevice/by-name/misc		flags=backup=1
/logo			emmc    /dev/block/platform/bootdevice/by-name/logo		flags=backup=1
/tee1			emmc	/dev/block/platform/bootdevice/by-name/tee1		flags=backup=1
/tee2			emmc	/dev/block/platform/bootdevice/by-name/tee2		flags=backup=1;subpartitionof=/tee1;backup=1
/vendor/nvcfg		ext4    /dev/block/platform/bootdevice/by-name/nvcfg		flags=backup=1
/nvram			emmc	/dev/block/platform/bootdevice/by-name/nvram		flags=backup=1	
/md1img			emmc    /dev/block/platform/bootdevice/by-name/md1img		flags=backup=1
/md1dsp			emmc    /dev/block/platform/bootdevice/by-name/md1dsp		flags=backup=1
/spmfw			emmc    /dev/block/platform/bootdevice/by-name/spmfw		flags=backup=1
/mcupmfw		emmc    /dev/block/platform/bootdevice/by-name/mcupmfw		flags=backup=1
/odmdtbo		emmc    /dev/block/platform/bootdevice/by-name/odmdtbo		flags=backup=1
/loader_ext1		emmc    /dev/block/platform/bootdevice/by-name/loader_ext1	flags=backup=1
/loader_ext2		emmc    /dev/block/platform/bootdevice/by-name/loader_ext2	flags=backup=1
/vendor/protect_f	ext4	/dev/block/platform/bootdevice/by-name/protect1		flags=backup=1
/vendor/protect_s	ext4	/dev/block/platform/bootdevice/by-name/protect2		flags=backup=1
/expdb			emmc	/dev/block/platform/bootdevice/by-name/expdb		flags=backup=1
/seccfg			emmc	/dev/block/platform/bootdevice/by-name/seccfg		flags=backup=1
/frp			emmc	/dev/block/platform/bootdevice/by-name/frp		flags=backup=1
/vendor/nvdata		ext4	/dev/block/platform/bootdevice/by-name/nvdata		flags=backup=1
/metadata		emmc	/dev/block/platform/bootdevice/by-name/metadata		flags=backup=1
/gz1			emmc	/dev/block/platform/bootdevice/by-name/gz1		flags=backup=1
/gz2			emmc	/dev/block/platform/bootdevice/by-name/gz2		flags=backup=1
/cache			ext4	/dev/block/platform/bootdevice/by-name/cache
/usbotg			auto    /dev/block/sda1		/dev/block/sda			flags=display="Usb-Otg";storage;removable;backup=0